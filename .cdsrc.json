{
  "[production]": {
    "folders": {
      "app": "ui_dist"
    }
  },
  "requires": {
    "auth": {
      "restrict_all_services": false,
      "impl": "cds-msal-auth",
      "[development]": {
        "kind": "jwt-auth"
      },
      "[test]": {
        "kind": "jwt-auth"
      },
      "[production]": {
        "kind": "jwt-auth"
      }
    },
    "db": {
      "[development]": {
        "kind": "postgres"
      },
      "[sqlite]": {
        "kind": "sqlite"
      },
      "[test]": {
        "kind": "postgres"
      },
      "[production]": {
        "kind": "postgres"
      }
    },
    "postgres": {
      "dialect": "plain",
      "impl": "cds-pg",
      "model": [
        "srv"
      ]
    },
    "uaa": {
      "kind": "xsuaa"
    }
  }
}
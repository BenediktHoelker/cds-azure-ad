    "axios": "^0.27.2",
    "azure-devops-node-api": "^11.2.0",
    "cds-msal-auth": "file:custom_packages/cds-msal-auth",
    "cds-pg": "^0.1.30",
    "cors": "^2.8.5",
    "custom-odata-v2-model": "file:custom_packages/custom-odata-v2-model",
    "didyoumean2": "^5.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "moment": "^2.29.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@sap/eslint-plugin-cds": "^2.4.1",
    "@sap/ux-specification": "^1.102.1",
    "@ui5/cli": "^2.14.9",
    "cds-pg-migra": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "sqlite3": "^5.0.9",
    "ui5-middleware-simpleproxy": "^0.9.1"
  },
  "scripts": {
    "start": "npm run set-default-env:dev && run-p start:cds start:ui5",
    "start:ui5": "ui5 serve",
    "start:cds": "cds run",
    "start:test": "npm run set-default-env:test && cross-env PORT=8080 cds run --profile test",
    "lint": "eslint srv && eslint app",
    "lint:fix:app": "eslint app --fix",
    "lint:fix:srv": "eslint srv --fix",
    "format": "prettier --write --plugin-search-dir=. .",
    "build": "cds build --production && npm run build:ui",
    "build:ui": "ui5 build self-contained --clean-dest --all",
    "build:ui:dev": "ui5 build dev --clean-dest --all",
    "deploy": "cds-pg-migra",
    "deploy:prod": "npm run set-default-env:prod && cds-pg-migra",
    "release": "npm run build:ui && npm run build",
    "docker:start:pg": "docker-compose -f docker-compose.yaml up -d",
    "set-default-env:dev": "bash ./set-env.sh development",
    "set-default-env:test": "bash ./set-env.sh test",
    "set-default-env:prod": "bash ./set-env.sh production"
  },
  "files": [
    "db",
    "srv",
    "app"
  ],
  "ui5": {
    "dependencies": [
      "ui5-middleware-simpleproxy",
      "@iot/ui5-errorhandler",
      "custom-odata-v2-model"
    ]
  },
  "cds": {
    "requires": {
      "microsoft.graph": {
        "kind": "odata",
        "model": "srv\\external\\msgraph",
        "credentials": {
          "url": "https://graph.microsoft.com/v1.0/me"
        }
      }
    }
  }
}
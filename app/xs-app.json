{
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/do/logout"
    },
    "routes": [
        {
            "source": "^/app/(.*)$",
            "target": "$1",
            "localDir": "./"
        },
        {
            "source": "^/admin/(.*)$",
            "target": "$1",
            "authenticationType": "xsuaa",
            "destination": "srv-api"
        }
    ]
}
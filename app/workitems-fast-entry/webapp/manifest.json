{
	"_version": "1.1.0",
	"sap.cloud": {
		"public": true,
		"service": "iot.project.planner"
	},
	"sap.app": {
		"id": "iot.workitemsfastentry",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"dataSources": {
			"mainService": {
				"uri": "v2/timetracking/",
				"type": "OData"
			}
		},
		"crossNavigation": {
			"inbounds": {
				"iot-workitemsfastentry-inbound": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "WorkItems",
					"action": "enter",
					"title": "{{appTitle}}",
					"subTitle": "{{appSubtitle}}",
					"icon": "sap-icon://add-activity"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "iot.workitemsfastentry.view.WorkItemsFastEntry",
			"type": "XML",
			"async": true,
			"id": "WorkItemsFastEntry"
		},
		"dependencies": {
			"minUI5Version": "1.75.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "iot.workitemsfastentry.i18n.i18n"
				}
			},
			"OData": {
				"dataSource": "mainService",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultUpdateMethod": "MERGE",
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"refreshAfterChange": false,
					"defaultCountMode": "None"
				}
			}
		},
		"": {
			"type": "sap.ui.model.json.JSONModel",
			"uri": "model/todoitems.json"
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "iot.workitemsfastentry.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteWorkItemsFastEntry",
					"pattern": "RouteWorkItemsFastEntry",
					"target": [
						"TargetWorkItemsFastEntry"
					]
				}
			],
			"targets": {
				"TargetWorkItemsFastEntry": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "WorkItemsFastEntry",
					"viewName": "WorkItemsFastEntry"
				}
			}
		}
	}
}
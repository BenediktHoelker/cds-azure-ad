{
	"_version": "1.1.0",
	"sap.cloud": {
		"public": true,
		"service": "iot.project.planner"
	},
	"sap.app": {
		"id": "iot.planner.components.workitems",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"dataSources": {
			"mainService": {
				"uri": "v2/timetracking/",
				"type": "OData"
			}
		},
		"crossNavigation": {
			"inbounds": {
				"iot-workitems-inbound": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "WorkItems",
					"action": "enter",
					"title": "{{appTitle}}",
					"subTitle": "{{appSubtitle}}",
					"icon": "sap-icon://add-activity"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"flexEnabled": false,
		"dependencies": {
			"minUI5Version": "1.75.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "iot.planner.components.workitems.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
		"rootView": {
			"viewName": "iot.planner.components.workitems.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "iot.planner.components.workitems.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "workitems",
					"target": "workitems"
				}
			],
			"targets": {
				"workitems": {
					"viewName": "WorkItems",
					"viewId": "wi",
					"viewLevel": 0,
					"title": "WorkItems",
					"viewType": "XML"
				}
			}
		}
	}
}
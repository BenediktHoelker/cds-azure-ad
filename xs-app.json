{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "/v2/analytics/(.*)$",
      "target": "/v2/analytics/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "/v2/timetracking/(.*)$",
      "target": "/v2/timetracking/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "/v2/admin/(.*)$",
      "target": "/v2/admin/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "/azure-devops/(.*)$",
      "target": "/azure-devops/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "/workitems/(.*)$",
      "target": "/workitems/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/index.html",
      "target": "index.html",
      "localDir": "./",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "localDir": "./app"
    }
  ]
}
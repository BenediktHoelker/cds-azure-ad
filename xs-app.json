{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/v2/timetracking/(.*)$",
      "target": "/v2/timetracking/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/timetracking/(.*)$",
      "target": "/timetracking/$1",
      "destination": "project-planning-dest",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/index.html",
      "target": "index.html",
      "localDir": "./",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    }
  ]
}